# Note:
# This Makefile is for GNU compiler in UNIX
# You have to change this for PC environment. Sorry.
#	
# 'makdfile.mak' is a sample Makefile in PC environment.
# It must help you to convert this Makefile
#

# Taget start address
TADDR	 = 80080000

# Compiler which you use
CC       = cc -EL -G 0 -O
CFLAGS   = -I$(INCDIR)
INCDIR   = /usr1/oka/PS-X/include
RM       = rm -f
RM_CMD   = $(RM) *.CKP *.ln *.BAK *.bak *.o core errs ,* *~ *.a .emacs_* tags TAGS make.log MakeOut
DEPEND   = makedepend

# library directory
LIBDIR   = /usr1/oka/PS-X/lib
LIBS     = $(LIBDIR)/libgpu.a \
	   $(LIBDIR)/libgte.a \
	   $(LIBDIR)/libetc.a \
	   $(LIBDIR)/libc.a \
	   $(LIBDIR)/libapi.a 

PROGRAM	= main

OBJS	= main.o

$(PROGRAM): $(OBJS)
	@ld -EL -N -T $(TADDR) -e main -o $(PROGRAM) $(OBJS) $(LIBS)
	@nm $(PROGRAM) | sort > $(PROGRAM).sym
	@aload $(PROGRAM)
	@squit

clean::
	$(RM) and

clean::
	$(RM_CMD) "#"*

depend::
	$(DEPEND) $(DEPENDFLAGS) -s "# DO NOT DELETE" -- $(CFLAGS) -- *.c

# -------------------------------------------------------------------------
# dependencies generated by makedepend

# DO NOT DELETE

